{
  "name": "react-flask-app",
  "version": "1.0.0",
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "@loadable/webpack-plugin": "^5.15.0",
    "assets-webpack-plugin": "^7.1.0",
    "babel-loader": "^8.2.2",
    "better-npm-run": "^0.1.1",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^5.0.1",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "null-loader": "^4.0.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^2.0.0",
    "webpack": "^5.42.0",
    "webpack-cli": "^4.4.0",
    "webpack-manifest-plugin": "^3.1.1",
    "webpack-merge": "^5.8.0",
    "webpack-modules": "^1.0.0",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@babel/core": "^7.13.15",
    "@loadable/component": "^5.15.0",
    "@loadable/server": "^5.15.0",
    "app-root-dir": "^1.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet-async": "^1.0.9",
    "react-router-dom": "^5.2.0",
    "react-ssr-prefetch": "^1.0.6",
    "serialize-javascript": "^6.0.0"
  },
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "build": "yarn clean && webpack",
    "start": "yarn build:production && yarn serve --reload --log-level=debug",
    "start:dev": "yarn build:dev && python src/app.py",
    "build:dev": "bnr build:dev",
    "build:production": "bnr build:production",
    "clean": "rm -rf dist && rm -rf src/server/renderer/build",
    "serve": "gunicorn src.app:app"
  },
  "betterScripts": {
    "build:dev": {
      "command": "yarn build",
      "env": {
        "NODE_ENV": "development",
        "APP_ENV": "development"
      }
    },
    "build:production": {
      "command": "yarn build",
      "env": {
        "NODE_ENV": "production",
        "APP_ENV": "production"
      }
    }
  }
}
